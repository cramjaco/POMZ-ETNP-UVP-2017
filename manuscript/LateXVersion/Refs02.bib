@article{bakkenBuoyantDensitiesDryMatter1983,
  title = {Buoyant {{Densities}} and {{Dry}}-{{Matter Contents}} of {{Microorganisms}}: {{Conversion}} of a {{Measured Biovolume}} into {{Biomass}}},
  shorttitle = {Buoyant {{Densities}} and {{Dry}}-{{Matter Contents}} of {{Microorganisms}}},
  author = {Bakken, Lars R. and Olsen, Rolf A.},
