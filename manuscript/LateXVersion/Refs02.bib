@article{kriestDifferentParameterizationsMarine2002,
  ids = {kriestDifferentParameterizationsMarine2002a},
  title = {Different Parameterizations of Marine Snow in a {{1D}}-Model and Their Influence on Representation of Marine Snow, Nitrogen Budget and Sedimentation},
