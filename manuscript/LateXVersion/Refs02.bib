@article{reynoldsDetectionEnterovirusesMarine1995,
  title = {Detection of Enteroviruses in Marine Waters by Direct {{RT}}-{{PCR}} and Cell Culture},
  author = {Reynolds, K. A. and Gerba, C. P. and Pepper, I. L.},
  date = {1995-01-01},
  journaltitle = {Water Science and Technology},
  shortjournal = {Water Science and Technology},
  volume = {31},
