@article{costelloAggregationDiatomBloom1995,
  title = {Aggregation of Diatom Bloom in a Mesocosm: {{Bulk}} and Individual Particle Optical Measurements},
  shorttitle = {Aggregation of Diatom Bloom in a Mesocosm},
  author = {Costello, David K. and Carder, Kendall L. and Hou, Weilin},
  date = {1995-01-01},
  journaltitle = {Deep Sea Research Part II: Topical Studies in Oceanography},
