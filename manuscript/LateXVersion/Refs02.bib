@article{georgeFlocculationHeavyMetals2007,
  title = {Flocculation, Heavy Metals ({{Cu}}, {{Pb}}, {{Zn}}) and the Sand–Mud Transition on the {{Adriatic}} Continental Shelf, {{Italy}}},
