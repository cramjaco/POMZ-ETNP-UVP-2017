@article{ofiteruCombinedNicheNeutral2010,
  title = {Combined Niche and Neutral Effects in a Microbial Wastewater Treatment Community},
  author = {Ofiţeru, Irina Dana and Lunn, Mary and Curtis, Thomas P. and Wells, George F. and Criddle, Craig S. and Francis, Christopher A. and Sloan, William T.},
  date = {2010-08-31},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {PNAS},
