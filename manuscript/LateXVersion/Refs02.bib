  eprinttype = {pmid},
  file = {/home/jacob/Zotero/storage/UE24SLDD/Gibbons et al. - 2013 - Evidence for a persistent microbial seed bank thro.pdf;/home/jacob/Zotero/storage/ZBGVKGP6/Gibbons et al. - 2013 - Evidence for a persistent microbial seed bank thro.pdf;/home/jacob/Zotero/storage/4WPSV6MV/4651.html;/home/jacob/Zotero/storage/U7MMJG7Z/4651.html},
  keywords = {deep sequencing,microbial ecology,Microbial ecology,rare biosphere},
  langid = {english},
