@article{simonsHighTurnoverFaecal2018,
  title = {High Turnover of Faecal Microbiome from Algal Feedstock Experimental Manipulations in the {{Pacific}} Oyster ({{Crassostrea}} Gigas)},
  author = {Simons, Ariel Levi and Churches, Nathan and Nuzhdin, Sergey},
  date = {2018},
  journaltitle = {Microbial Biotechnology},
  volume = {11},
  pages = {848--858},
