@article{alldredgeCharacteristicsDynamicsSignificance1988,
  title = {Characteristics, Dynamics and Significance of Marine Snow},
  author = {Alldredge, Alice L. and Silver, Mary W.},
  date = {1988},
  journaltitle = {Progress in Oceanography},
  shortjournal = {Progress in Oceanography},
  volume = {20},
