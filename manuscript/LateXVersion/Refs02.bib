  author = {Jackson, George A. and Burd, Adrian B.},
  date = {2015-04},
  journaltitle = {Progress in Oceanography},
  shortjournal = {Progress in Oceanography},
  volume = {133},
  pages = {55--65},
  issn = {0079-6611},
  doi = {10.1016/j.pocean.2014.08.014},
  url = {http://www.sciencedirect.com/science/article/pii/S0079661114001451},
  urldate = {2015-07-07},
  abstract = {The dynamics of elements in the water column is complex, depending on multiple biological and physical processes operating at very different physical scales. Coagulation of particulate material is important for transforming particles and moving them in the water column. Mechanistic models of coagulation processes provide a means to predict these processes, help interpret observations, and provide insight into the processes occurring. However, most model applications have focused on describing simple marine systems and mechanisms. We argue that further model development, in close collaboration with field and experimental scientists, is required in order to extend the models to describe the large-scale elemental distributions and interactions being studied as part of GEOTRACES. Models that provide a fundamental description of trace element–particle interactions are required as are experimental tests of the mechanisms involved and the predictions arising from models. However, a comparison between simple and complicated models of aggregation and trace metal provides a means for understanding the implications of simplifying assumptions and providing guidance as to which simplifications are needed.},
  file = {/home/jacob/Zotero/storage/PKGSNVSA/Jackson and Burd - 2015 - Simulating aggregate dynamics in ocean biogeochemi.pdf;/home/jacob/Zotero/storage/GB2C7TAA/S0079661114001451.html},
  series = {{{GEOTRACES Synthesis}} and {{Modeling}}: {{The}} Role of Particles in the Marine Biogeochemical Cycles of Trace Elements and Their Isotopes}
}

