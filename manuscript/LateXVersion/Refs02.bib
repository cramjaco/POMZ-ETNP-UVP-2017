@article{hongLinkingSeasonalInorganic2014,
  title = {Linking Seasonal Inorganic Nitrogen Shift to the Dynamics of Microbial Communities in the {{Chesapeake Bay}}},
  author = {Hong, Yiguo and Xu, Xiongrong and Kan, Jinjun and Chen, Feng},
  date = {2014-04-01},
  journaltitle = {Applied Microbiology and Biotechnology},
  shortjournal = {Appl Microbiol Biotechnol},
  volume = {98},
