  publisher = {{American Society for Microbiology}},
  issn = {0099-2240, 1098-5336},
  url = {https://aem.asm.org/content/32/2/245},
  urldate = {2020-06-29},
  abstract = {Studies of the effects of temperature and salinity on the survival of three enteric viruses (poliomyelitis type 1, echovirus-6, and coxsackievirus B-5) under controlled laboratory conditions and in situ indicate that temperature rather than salinity is the critical factor affecting their stability, in that the higher the temperature the more rapid was the loss of viral infectivity. In the laboratory studies, all three viruses were quite stable at 4 degrees C, with infectious virus still detectable after 46 weeks of incubation. In situ studies on virus survival in free-flowing estuarine or marine waters showed that, although the viruses were more labile in natural waters than in the laboratory studies, they persisted for several months, in some cases during the winter months. At all temperatures and salinities, coxsackievirus B-5 was the most stable, echovirus-6 was intermediate, and poliovirus 1 was the least stable of the viruses tested.},
  eprint = {184736},
  eprinttype = {pmid},
  file = {/home/jacob/Zotero/storage/HU366NC7/Lo et al. - 1976 - Stability of human enteroviruses in estuarine and .pdf;/home/jacob/Zotero/storage/X3XXGVSI/Lo et al. - 1976 - Stability of human enteroviruses in estuarine and .pdf;/home/jacob/Zotero/storage/A4CTKGWU/245.html;/home/jacob/Zotero/storage/GR3UXJ8Z/245.html},
  keywords = {Waterborne illness},
  langid = {english},
  number = {2}
}

