@article{newmanModularityCommunityStructure2006,
  title = {Modularity and Community Structure in Networks},
  author = {Newman, M. E. J.},
  date = {2006-06-06},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {PNAS},
  volume = {103},
