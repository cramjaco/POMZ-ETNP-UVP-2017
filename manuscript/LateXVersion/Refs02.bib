@article{picheralUnderwaterVisionProfiler2010,
  title = {The {{Underwater Vision Profiler}} 5: {{An}} Advanced Instrument for High Spatial Resolution Studies of Particle Size Spectra and Zooplankton},
  shorttitle = {The {{Underwater Vision Profiler}} 5},
  author = {Picheral, Marc and Guidi, Lionel and Stemmann, Lars and Karl, David M. and Iddaoud, Ghizlaine and Gorsky, Gabriel},
