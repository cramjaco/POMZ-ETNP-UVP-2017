@article{eubigAcuteRenalFailure2005,
  title = {Acute {{Renal Failure}} in {{Dogs After}} the {{Ingestion}} of {{Grapes}} or {{Raisins}}: {{A Retrospective Evaluation}} of 43 {{Dogs}} (1992–2002)},
  shorttitle = {Acute {{Renal Failure}} in {{Dogs After}} the {{Ingestion}} of {{Grapes}} or {{Raisins}}},
