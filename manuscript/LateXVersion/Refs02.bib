@article{wrightNovelDeltasubdivisionProteobacterial1997,
  title = {A Novel Delta-Subdivision Proteobacterial Lineage from the Lower Ocean Surface Layer.},
  author = {Wright, T. D. and Vergin, K. L. and Boyd, P. W. and Giovannoni, S. J.},
