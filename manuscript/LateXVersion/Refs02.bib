  author = {Swan, Brandon K. and Martinez-Garcia, Manuel and Preston, Christina M. and Sczyrba, Alexander and Woyke, Tanja and Lamy, Dominique and Reinthaler, Thomas and Poulton, Nicole J. and Masland, E. Dashiell P. and Gomez, Monica Lluesma and Sieracki, Michael E. and DeLong, Edward F. and Herndl, Gerhard J. and Stepanauskas, Ramunas},
