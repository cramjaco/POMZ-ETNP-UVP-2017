@article{hidalgoOntogeneticVerticalDistribution2005,
  title = {Ontogenetic Vertical Distribution and Diel Migration of the Copepod {{Eucalanus}} Inermis in the Oxygen Minimum Zone off Northern {{Chile}} (20–21° {{S}})},
  author = {Hidalgo, Pamela and Escribano, Ruben and Morales, Carmen E.},
  date = {2005-06-01},
  journaltitle = {Journal of Plankton Research},
  shortjournal = {Journal of Plankton Research},
  volume = {27},
  pages = {519--529},
  issn = {0142-7873},
  doi = {10.1093/plankt/fbi025},
  url = {https://doi.org/10.1093/plankt/fbi025},
  urldate = {2021-01-12},
  abstract = {The vertical and ontogenetic distribution, and diel vertical migration (DVM), of Eucalanus inermis in relation to the strong vertical gradient in oxygen concentration associated with an intense oxygen minimum zone (OMZ) were studied at a coastal area off northern Chile (20–21° S). A close relationship between the abundance of the whole copepod population and low oxygen waters was found, with most developmental stages remaining near the base of the oxycline (30–80 m) and within the upper zone of the OMZ (30–200 m). All stages performed DVM but not at all the stations, mainly between the 30–60 and 60–200 m strata; a small fraction (\&lt;20\%) appeared in the surface layer (0–30 m) mostly at night. This strategy of movement would result in a better utilization of food resources since the strong physical and chemical gradients at the base of the oxycline and upper OMZ boundary might serve as a site of particle accumulation. A secondary fluorescence peak was, in fact, found at all the stations, coinciding with minimal dissolved oxygen (DO, \&lt;1 mL O2 L−1) at the base of the oxycline or in the upper OMZ boundary. The relevance of the biogeochemical flux involved in this diel migration pattern was assessed by calculating the potential active input of carbon and nitrogen from the upper layers into deeper the OMZ.},
  file = {/home/jacob/Zotero/storage/LZ7YH3JX/Hidalgo et al. - 2005 - Ontogenetic vertical distribution and diel migrati.pdf;/home/jacob/Zotero/storage/3UMEM4KG/1495412.html},
  number = {6}
}

