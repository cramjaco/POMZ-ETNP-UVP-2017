  title = {Bioaccumulation of Microcystins in the Food Web: A Field Study of Four {{Swedish}} Lakes},
  shorttitle = {Bioaccumulation of Microcystins in the Food Web},
  author = {Larson, Daniel and Ahlgren, Gunnel and Willén, Eva},
