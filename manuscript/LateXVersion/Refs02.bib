  keywords = {algae,Biofilm,Biofuel,chlamydomonas-reinhardtii,coupled respiration,cultivation,diatom phaeodactylum-tricornutum,growth,lipid-accumulation,Microalgae,oxygen microsensors,Photosynthesis,temperature,triacylglycerol,Wastewater remediation},
  langid = {english}
}

