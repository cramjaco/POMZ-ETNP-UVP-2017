  title = {The {{Divergent AmoC}}(3) {{Subunit}} of {{Ammonia Monooxygenase Functions}} as {{Part}}   of a {{Stress Response System}} in {{Nitrosomonas}} Europaea},
  author = {Berube, Paul M. and Stahl, David A.},
