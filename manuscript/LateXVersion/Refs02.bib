  author = {De Corte, Daniele and Srivastava, Abhishek and Koski, Marja and Garcia, Juan Antonio L. and Takaki, Yoshihiro and Yokokawa, Taichi and Nunoura, Takuro and Elisabeth, Nathalie H. and Sintes, Eva and Herndl, Gerhard J.},
