@article{thingstadElementsTheoryMechanisms2000,
  title = {Elements of a {{Theory}} for the {{Mechanisms Controlling Abundance}}, {{Diversity}}, and {{Biogeochemical Role}} of {{Lytic Bacterial Viruses}} in {{Aquatic Systems}}},
