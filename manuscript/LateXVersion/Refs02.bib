@article{wellsObservationsGeologicSignificance1987,
  title = {Observations and Geologic Significance of Marine Snow in a Shallow-Water, Partially Enclosed Marine Embayment},
  author = {Wells, J. T. and Shanks, A. L.},
  date = {1987},
  journaltitle = {Journal of Geophysical Research: Oceans},
  volume = {92},
  pages = {13185--13190},
