  shorttitle = {{{KERFIX}}, a Time-Series Station in the {{Southern Ocean}}},
  author = {Jeandel, Catherine and Ruiz-Pino, Diana and Gjata, Elvis and Poisson, Alain and Brunet, Christian and Charriaud, Edwige and Dehairs, Frank and Delille, Daniel and Fiala, Michel and Fravalo, Charles and Carlos Miquel, Juan and Park, Young-Hyang and Pondaven, Philippe and Quéguiner, Bernard and Razouls, Suzanne and Shauer, Bernard and Tréguer, Paul},
