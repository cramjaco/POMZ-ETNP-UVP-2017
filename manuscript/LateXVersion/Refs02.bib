  author = {Céa, B. and Lefèvre, D. and Chirurgien, L. and Raimbault, P. and Garcia, N. and Charrière, B. and Grégori, G. and Ghiglione, J. F. and Barani, A. and Lafont, M. and Wambeke, F. Van},
  date = {2014-09-14},
  journaltitle = {Environmental Science and Pollution Research},
  shortjournal = {Environ Sci Pollut Res},
  pages = {1--15},
  issn = {0944-1344, 1614-7499},
  doi = {10.1007/s11356-014-3500-9},
  url = {http://link.springer.com.libproxy.usc.edu/article/10.1007/s11356-014-3500-9},
  urldate = {2014-09-16},
  abstract = {We simultaneously measured bacterial production (BP), bacterial respiration (BR), alkaline phosphatase activity (phos) and ectoaminopeptidase activity (prot) in relation to biogeochemical parameters, nutritive resources and in situ temperature over a 1-year survey at the long-term observatory the SOLEMIO station (Marseille bay, NW Mediterranean Sea). Despite its proximity to the coast, oligotrophic conditions prevailed at this station (yearly mean of Chl a = 0.43 μg dm−3, NO3 = 0.55 μmol dm−3 and PO4 = 0.04 μmol dm−3). Episodic meteorological events (dominant winds, inputs from the Rhone River) induced rapid oscillations (within 15 days) in temperature and sometimes salinity that resulted in rapid changes in phytoplankton succession and a high variability in C/P ratios within the particulate and dissolved organic matter. Throughout the year, BP ranged from 0.01 to 0.82 μg C dm−3 h−1 and bacterial growth efficiency varied from 1 to 39 \%, with higher values in summer. Enrichment experiments showed that BP was limited most of the year by phosphorus availability (except in winter). A significant positive correlation was found between in situ temperature, BP, BR and phos. Finally, we found that temperature and phosphate availability were the main factors driving heterotrophic bacterial activity and thus play a fundamental role in carbon fluxes within the marine ecosystem.},
  file = {/home/jacob/Zotero/storage/ZECGVQZ9/Céa et al. - 2014 - An annual survey of bacterial production, respirat.pdf;/home/jacob/Zotero/storage/JMC9R7EG/s11356-014-3500-9.html},
  keywords = {Atmospheric Protection/Air Quality Control/Air Pollution,Bacterial growth efficiency,Bacterial production,Bacterial respiration,Ecotoxicology,Ectoenzymes,Environment; general,Environmental Chemistry,Environmental Health,nw mediterranean sea,phosphorus limitation,SOLEMIO,Waste Water Technology / Water Pollution Control / Water Management / Aquatic Pollution},
  langid = {english}
}

@article{cepeda-moralesEffectOxygenMinimum2009,
  title = {Effect of the Oxygen Minimum Zone on the Second Chlorophyll Maximum in the {{Eastern Tropical Pacific}} off {{Mexico}}},
  author = {Cepeda-Morales, J. and Beier, E. and Gaxiola-Castro, G. and Lavin, M. F and Godinez, V. M},
  date = {2009-12},
