@article{sollerIncidenceGastrointestinalIllness2017a,
  title = {Incidence of Gastrointestinal Illness Following Wet Weather Recreational Exposures: {{Harmonization}} of Quantitative Microbial Risk Assessment with an Epidemiologic Investigation of Surfers},
  shorttitle = {Incidence of Gastrointestinal Illness Following Wet Weather Recreational Exposures},
