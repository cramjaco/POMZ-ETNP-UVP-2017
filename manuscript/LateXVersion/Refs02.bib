  author = {Bauer, Margarete and Kube, Michael and Teeling, Hanno and Richter, Michael and Lombardot, Thierry and Allers, Elke and Würdemann, Chris A. and Quast, Christian and Kuhl, Heiner and Knaust, Florian and Woebken, Dagmar and Bischof, Kerstin and Mussmann, Marc and Choudhuri, Jomuna V. and Meyer, Folker and Reinhardt, Richard and Amann, Rudolf I. and Glöckner, Frank Oliver},
  date = {2006},
  journaltitle = {Environmental Microbiology},
