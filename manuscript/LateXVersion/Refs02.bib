@article{lingDrinkingWaterMicrobiome2018,
  title = {Drinking Water Microbiome Assembly Induced by Water Stagnation},
  author = {Ling, Fangqiong and Whitaker, Rachel and LeChevallier, Mark W. and Liu, Wen-Tso},
  date = {2018-03-27},
  journaltitle = {The ISME Journal},
  pages = {1},
  issn = {1751-7370},
  doi = {10.1038/s41396-018-0101-5},
  url = {https://www.nature.com/articles/s41396-018-0101-5},
  urldate = {2018-03-28},
  abstract = {What happens to tap water when you are away from home? Day-to-day water stagnation in building plumbing can potentially result in water quality deterioration (e.g., lead release or pathogen proliferation), which is a major public health concern. However, little is known about the microbial ecosystem processes in plumbing systems, hindering the development of biological monitoring strategies. Here, we track tap water microbiome assembly in situ, showing that bacterial community composition changes rapidly from the city supply following \textasciitilde 6-day stagnation, along with an increase in cell count from 103 cells/mL to upwards of 7.8\,×\,105 cells/mL. Remarkably, bacterial community assembly was highly reproducible in this built environment system (median Spearman correlation between temporal replicates\,=\,0.78). Using an island biogeography model, we show that neutral processes arising from the microbial communities in the city water supply (i.e., migration and demographic stochasticity) explained the island community composition in proximal pipes (Goodness-of-fit\,=\,0.48), yet declined as water approached the faucet (Goodness-of-fit\,=\,0.21). We developed a size-effect model to simulate this process, which indicated that pipe diameter drove these changes by mediating the kinetics of hypochlorite decay and cell detachment, affecting selection, migration, and demographic stochasticity. Our study challenges current water quality monitoring practice worldwide which ignore biological growth in plumbing, and suggests the island biogeography model as a useful framework to evaluate building water system quality.},
  file = {/home/jacob/Zotero/storage/58A2WYM3/Ling et al. - 2018 - Drinking water microbiome assembly induced by wate.pdf},
  langid = {english}
}

@article{liPhosphorusAvailabilityTurnover2017,
  title = {Phosphorus Availability and Turnover in the {{Chesapeake Bay}}: {{Insights}} from Nutrient Stoichiometry and Phosphate Oxygen Isotope Ratios: {{Phosphorus Cycling}} in the {{Chesapeake Bay}}},
