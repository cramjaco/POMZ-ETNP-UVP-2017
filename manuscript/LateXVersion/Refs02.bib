@article{ebersbachSinkingParticleProperties2008,
  title = {Sinking Particle Properties from Polyacrylamide Gels during the {{KErguelen Ocean}} and {{Plateau}} Compared {{Study}} ({{KEOPS}}): {{Zooplankton}} Control of Carbon Export in an Area of Persistent Natural Iron Inputs in the {{Southern Ocean}}},
