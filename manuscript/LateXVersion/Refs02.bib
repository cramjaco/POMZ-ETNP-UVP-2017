@article{saitoComplexationCobaltNatural2001,
  title = {Complexation of Cobalt by Natural Organic Ligands in the {{Sargasso Sea}} as Determined by a New High-Sensitivity Electrochemical Cobalt Speciation Method Suitable for Open Ocean Work},
  author = {Saito, Mak A. and Moffett, James W.},
