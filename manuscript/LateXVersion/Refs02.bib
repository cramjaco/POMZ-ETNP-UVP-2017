@article{weberDeepOceanNutrients2016,
  title = {Deep Ocean Nutrients Imply Large Latitudinal Variation in Particle Transfer Efficiency},
  author = {Weber, Thomas and Cram, Jacob A. and Leung, Shirley W. and DeVries, Timothy and Deutsch, Curtis},
  date = {2016-08-02},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {PNAS},
  volume = {113},
