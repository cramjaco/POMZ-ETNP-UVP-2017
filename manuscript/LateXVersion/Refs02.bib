}

@article{campoTamingSmallestPredators2013,
  title = {Taming the Smallest Predators of the Oceans},
  author = {del Campo, Javier and Not, Fabrice and Forn, Irene and Sieracki, Michael E. and Massana, Ramon},
  date = {2013},
  journaltitle = {The ISME Journal},
  shortjournal = {ISME J},
  volume = {7},
