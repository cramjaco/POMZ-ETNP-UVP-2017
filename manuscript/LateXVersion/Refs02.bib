  keywords = {aggregation,coagulation,fractal,heterodisperse distribution,Larry Sanford,particle size spectra,sectional approach,simulations},
  number = {1}
}

@article{jacobsenRoleBacteriaCopepod1984,
  title = {Role of {{Bacteria}} in {{Copepod Fecal Pellet Decomposition}}: {{Colonization}}, {{Growth Rates}} and {{Mineralization}}},
