@article{montesDecadalVariabilityOxygen2016,
  title = {Decadal Variability in the Oxygen Inventory of {{North Atlantic}} Subtropical Underwater Captured by Sustained, Long-Term Oceanographic Time Series Observations: {{Oxygen Variability}} in {{North Atlantic STUW}}},
  shorttitle = {Decadal Variability in the Oxygen Inventory of {{North Atlantic}} Subtropical Underwater Captured by Sustained, Long-Term Oceanographic Time Series Observations},
  author = {Montes, Enrique and Muller-Karger, Frank E. and Cianca, Andrés and Lomas, Michael W. and Lorenzoni, Laura and Habtes, Sennai},
