@article{mitchellExplainingVariationSoil2012,
  title = {Explaining the Variation in the Soil Microbial Community: Do Vegetation Composition and Soil Chemistry Explain the Same or Different Parts of the Microbial Variation?},
  shorttitle = {Explaining the Variation in the Soil Microbial Community},
  author = {Mitchell, Ruth Joy and Hester, Alison J. and Campbell, Colin D. and Chapman, Stephen J. and Cameron, Clare M. and Hewison, Richard L. and Potts, Jackie M.},
  date = {2012-02-01},
  journaltitle = {Plant and Soil},
  shortjournal = {Plant Soil},
  volume = {351},
  pages = {355--362},
  issn = {0032-079X, 1573-5036},
  doi = {10.1007/s11104-011-0968-7},
  url = {http://link.springer.com/article/10.1007/s11104-011-0968-7},
  urldate = {2013-01-18},
  abstract = {Aim To assess whether vegetation composition and soil chemistry explain the same or different parts of the variation in the soil microbial community (SMC). Method The above and below-ground communities and soil chemical properties were studied along a successional gradient from moorland to deciduous woodland. The SMC was assessed using PLFAs and M-TRFLPs. Using variance partitioning, Co-Correspondence Analysis (CoCA) and Canonical Correspondence Analysis (CCA), the variation (total inertia) in the SMC was partitioned into variation which was uniquely explained by either plant composition or soil chemistry, variation explained by both soil chemistry and plant composition, and unexplained variation. Results Plant community composition uniquely explained 30, 13, 16 and 20\% of the inertia and soil chemistry uniquely explained 5, 18, 9 and 9\% of the inertia in the archaeal TRFLPs, bacterial TRFLPs, fungal TRFLPs and all PLFAs, respectively. Conclusion For the first time, variance partitioning was used to include data from a CoCA; although the current limits of such an approach are shown, this study illustrates the potential of such analyses and shows that soil chemistry and plant composition are, in substantial amounts, explaining different parts of the variation within the SMC. This marks an important step in furthering our understanding of the relative importance of different drivers of change in the SMC.},
  file = {/home/jacob/Zotero/storage/S8X8227F/Mitchell et al. - 2012 - Explaining the variation in the soil microbial com.pdf;/home/jacob/Zotero/storage/X2ASDKX4/10.html},
  keywords = {Co-correspondence analysis,ecology,Ecosystem engineer,Moorland,Plant Physiology,Plant Sciences,PLFA,Soil Science & Conservation,Succession,TRFLP,Variance partitioning},
  langid = {english},
  number = {1-2}
}

@article{mitchellVegetationCompositionSoil2010,
  title = {Is Vegetation Composition or Soil Chemistry the Best Predictor of the Soil Microbial Community?},
  author = {Mitchell, Ruth J. and Hester, Alison J. and Campbell, Colin D. and Chapman, Stephen J. and Cameron, Clare M. and Hewison, Richard L. and Potts, Jackie M.},
