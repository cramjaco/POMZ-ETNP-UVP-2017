@article{mcdonnellNewMethodEstimation2012,
  title = {A New Method for the Estimation of Sinking Particle Fluxes from Measurements of the Particle Size Distribution, Average Sinking Velocity, and Carbon Content},
  author = {McDonnell, Andrew M. P. and Buesseler, Ken O.},
  date = {2012-05-01},
  journaltitle = {Limnology and Oceanography: Methods},
  shortjournal = {Limnol. Oceanogr. Methods},
