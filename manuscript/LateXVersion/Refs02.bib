  urldate = {2013-11-15},
  abstract = {Oxygen minimum zones, also known as oceanic “dead zones,” are widespread oceanographic features currently expanding because of global warming. Although inhospitable to metazoan life, they support a cryptic microbiota whose metabolic activities affect nutrient and trace gas cycling within the global ocean. Here, we report metagenomic analyses of a ubiquitous and abundant but uncultivated oxygen minimum zone microbe (SUP05) related to chemoautotrophic gill symbionts of deep-sea clams and mussels. The SUP05 metagenome harbors a versatile repertoire of genes mediating autotrophic carbon assimilation, sulfur oxidation, and nitrate respiration responsive to a wide range of water-column redox states. Our analysis provides a genomic foundation for understanding the ecological and biogeochemical role of pelagic SUP05 in oxygen-deficient oceanic waters and its potential sensitivity to environmental changes.},
  eprint = {19900896},
  eprinttype = {pmid},
  file = {/home/jacob/Zotero/storage/K5VVFRSQ/Walsh et al. - 2009 - Metagenome of a Versatile Chemolithoautotroph from.pdf;/home/jacob/Zotero/storage/RH9ZHBED/578.html},
