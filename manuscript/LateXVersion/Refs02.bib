@article{nobleComparisonTotalColiform2003,
  ids = {nobleComparisonTotalColiform2003a},
  title = {Comparison of Total Coliform, Fecal Coliform, and Enterococcus Bacterial Indicator Response for Ocean Recreational Water Quality Testing},
  author = {Noble, R. T and Moore, D. F and Leecaster, M. K and McGee, C. D and Weisberg, S. B},
  date = {2003-04-01},
