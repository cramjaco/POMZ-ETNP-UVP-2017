  author = {Fouilland, Eric and Tolosa, Imma and Bonnet, Delphine and Bouvier, Corinne and Bouvier, Thierry and Bouvy, Marc and Got, Patrice and Le Floc'h, Emilie and Mostajir, Behzad and Roques, Cécile and Sempéré, Richard and Sime-Ngando, Télesphore and Vidussi, Francesca},
