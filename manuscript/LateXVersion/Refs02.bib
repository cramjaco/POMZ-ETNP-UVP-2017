@article{condonJellyfishBloomsResult2011,
  title = {Jellyfish Blooms Result in a Major Microbial Respiratory Sink of Carbon in Marine Systems},
  author = {Condon, Robert H. and Steinberg, Deborah K. and del Giorgio, Paul A. and Bouvier, Thierry C. and Bronk, Deborah A. and Graham, William M. and Ducklow, Hugh W.},
  date = {2011-06-21},
  journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
  shortjournal = {Proc Natl Acad Sci U S A},
  volume = {108},
  pages = {10225--10230},
  issn = {0027-8424},
  doi = {10.1073/pnas.1015782108},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3121804/},
  urldate = {2018-10-19},
  abstract = {Jellyfish blooms occur in many estuarine and coastal regions and may be increasing in their magnitude and extent worldwide. Voracious jellyfish predation impacts food webs by converting large quantities of carbon (C), fixed by primary producers and consumed by secondary producers, into gelatinous biomass, which restricts C transfer to higher trophic levels because jellyfish are not readily consumed by other predators. In addition, jellyfish release colloidal and dissolved organic matter (jelly-DOM), and could further influence the functioning of coastal systems by altering microbial nutrient and DOM pathways, yet the links between jellyfish and bacterioplankton metabolism and community structure are unknown. Here we report that jellyfish released substantial quantities of extremely labile C-rich DOM, relative to nitrogen (25.6 ± 31.6 C:1N), which was quickly metabolized by bacterioplankton at uptake rates two to six times that of bulk DOM pools. When jelly-DOM was consumed it was shunted toward bacterial respiration rather than production, significantly reducing bacterial growth efficiencies by 10\% to 15\%. Jelly-DOM also favored the rapid growth and dominance of specific bacterial phylogenetic groups (primarily γ-proteobacteria) that were rare in ambient waters, implying that jelly-DOM was channeled through a small component of the in situ microbial assemblage and thus induced large changes in community composition. Our findings suggest major shifts in microbial structure and function associated with jellyfish blooms, and a large detour of C toward bacterial CO2 production and away from higher trophic levels. These results further suggest fundamental transformations in the biogeochemical functioning and biological structure of food webs associated with jellyfish blooms.},
  eprint = {21646531},
  eprinttype = {pmid},
  file = {/home/jacob/Zotero/storage/28DKT3YC/Condon et al. - 2011 - Jellyfish blooms result in a major microbial respi.pdf},
  keywords = {Chesapeake Bay,jellyfish},
  number = {25},
  options = {useprefix=true},
  pmcid = {PMC3121804}
}

