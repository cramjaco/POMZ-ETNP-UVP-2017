  author = {Guidi, Lionel and Stemmann, Lars and Jackson, George A. and Ibanez, Frédéric and Claustre, Hervé and Legendre, Louis and Picheral, Marc and Gorskya, Gabriel},
  date = {2009-11-01},
  journaltitle = {Limnology and Oceanography},
  shortjournal = {Limnol. Oceanogr.},
