@article{consortiumStructureFunctionDiversity2012,
  title = {Structure, Function and Diversity of the Healthy Human Microbiome},
  author = {Consortium, The Human Microbiome Project},
  date = {2012-06-14},
  journaltitle = {Nature},
  volume = {486},
