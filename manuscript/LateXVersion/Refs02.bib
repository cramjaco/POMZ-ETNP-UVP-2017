@article{rakauskasIdentificationDifferencesRural2009,
  title = {Identification of Differences between Rural and Urban Safety Cultures},
  author = {Rakauskas, Michael E. and Ward, Nicholas J. and Gerberich, Susan G.},
  date = {2009-09-01},
  journaltitle = {Accident Analysis \& Prevention},
  shortjournal = {Accident Analysis \& Prevention},
  volume = {41},
