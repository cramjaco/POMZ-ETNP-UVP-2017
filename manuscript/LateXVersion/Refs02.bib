@article{patelVirusProkaryoteEnumeration2007,
  title = {Virus and Prokaryote Enumeration from Planktonic Aquatic Environments by Epifluorescence Microscopy with {{SYBR Green I}}},
  author = {Patel, Anand and Noble, Rachel T. and Steele, Joshua A. and Schwalbach, Michael S. and Hewson, Ian and Fuhrman, Jed A.},
