}

@article{pierceInfluenceSiteSeason2016,
  title = {The Influence of Site and Season on the Gut and Pallial Fluid Microbial Communities of the Eastern Oyster, {{Crassostrea}} Virginica ({{Bivalvia}}, {{Ostreidae}}): Community-Level Physiological Profiling and Genetic Structure},
  shorttitle = {The Influence of Site and Season on the Gut and Pallial Fluid Microbial Communities of the Eastern Oyster, {{Crassostrea}} Virginica ({{Bivalvia}}, {{Ostreidae}})},
