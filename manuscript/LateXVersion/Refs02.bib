}

@article{kunihiroPhospholipidDerivedFattyAcids2014,
  title = {Phospholipid-{{Derived Fatty Acids}} and {{Quinones}} as {{Markers}} for {{Bacterial Biomass}} and {{Community Structure}} in {{Marine Sediments}}},
  author = {Kunihiro, Tadao and Veuger, Bart and Vasquez-Cardenas, Diana and Pozzato, Lara and Guitton, Marie Le and Moriya, Kazuyoshi and Kuwae, Michinobu and Omori, Koji and Boschker, Henricus T. S. and van Oevelen, Dick},
  date = {2014-04-25},
  journaltitle = {PLOS ONE},
