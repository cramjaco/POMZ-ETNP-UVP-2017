@article{ducklowChesapeakeBayNutrient1982,
  title = {Chesapeake {{Bay}} Nutrient and Plankton Dynamics. 1. {{Bacterial}} Biomass and Production during Spring Tidal Destratification in the {{York River}}, {{Virginia}}, Estuary1,2},
