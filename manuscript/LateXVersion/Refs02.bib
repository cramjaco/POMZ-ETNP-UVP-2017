  keywords = {agricultural soil,ammonia oxidation,Bacteria,cell-division,marine   crenarchaeota,mesophilic crenarchaeota,nitrification,nitrogen cycle,nitrosopumilus-maritimus,ocean,oxidation,planktonic archaea,terrestrial hot-springs,Thaumarchaeota},
