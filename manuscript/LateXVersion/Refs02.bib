}

@article{adriaenssensUsingSignatureGenes2014,
  title = {Using {{Signature Genes}} as {{Tools To Assess Environmental Viral Ecology}} and {{Diversity}}},
  author = {Adriaenssens, Evelien M. and Cowan, Don A.},
  date = {2014-08},
  journaltitle = {Applied and Environmental Microbiology},
