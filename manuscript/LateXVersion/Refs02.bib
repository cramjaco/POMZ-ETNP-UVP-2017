@article{deuserSeasonalInterannualVariations1986,
  title = {Seasonal and Interannual Variations in Deep-Water Particle Fluxes in the {{Sargasso Sea}} and Their Relation to Surface Hydrography},
  author = {Deuser, W. G.},
  date = {1986-02-01},
  journaltitle = {Deep Sea Research Part A. Oceanographic Research Papers},
