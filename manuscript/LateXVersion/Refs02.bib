@article{loStabilityHumanEnteroviruses1976,
  ids = {loStabilityHumanEnteroviruses1976a},
  title = {Stability of Human Enteroviruses in Estuarine and Marine Waters.},
  author = {Lo, S. and Gilbert, J. and Hetrick, F.},
  date = {1976-08-01},
  journaltitle = {Applied and Environmental Microbiology},
