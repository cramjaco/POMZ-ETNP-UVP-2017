@article{sheikMetabolicFlexibilityEnigmatic2014,
  title = {Metabolic Flexibility of Enigmatic {{SAR324}} Revealed through Metagenomics and Metatranscriptomics},
  author = {Sheik, Cody S. and Jain, Sunit and Dick, Gregory J.},
  date = {2014-01-01},
  journaltitle = {Environmental Microbiology},
  shortjournal = {Environ Microbiol},
  volume = {16},
  pages = {304--317},
  issn = {1462-2920},
  doi = {10.1111/1462-2920.12165},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/1462-2920.12165/abstract},
  urldate = {2014-09-05},
  abstract = {Chemolithotrophy is a pervasive metabolic lifestyle for microorganisms in the dark ocean. The SAR324 group of Deltaproteobacteria is ubiquitous in the ocean and has been implicated in sulfur oxidation and carbon fixation, but also contains genomic signatures of C1 utilization and heterotrophy. Here, we reconstructed the metagenome and metatranscriptome of a population of SAR324 from a hydrothermal plume and surrounding waters in the deep Gulf of California to gain insight into the genetic capability and transcriptional dynamics of this enigmatic group. SAR324's metabolism is signified by genes that encode a novel particulate hydrocarbon monooxygenase (pHMO), degradation pathways for corresponding alcohols and short-chain fatty acids, dissimilatory sulfur oxidation, formate dehydrogenase (FDH) and a nitrite reductase (NirK). Transcripts of the pHMO, NirK, FDH and transporters for exogenous carbon and amino acid uptake were highly abundant in plume waters. Sulfur oxidation genes were also abundant in the plume metatranscriptome, indicating SAR324 may also utilize reduced sulfur species in hydrothermal fluids. These results suggest that aspects of SAR324's versatile metabolism (lithotrophy, heterotrophy and alkane oxidation) operate simultaneously, and may explain SAR324's ubiquity in the deep Gulf of California and in the global marine biosphere.},
  langid = {english},
  number = {1}
}

@article{shelfordReducedAmmoniumRemineralisation,
  title = {Reduced Ammonium Remineralisation by Heterotrophic Bacteria with Virus Removal},
  author = {Shelford, E J and Middelboe, M and Suttle, C},
  journaltitle = {Proceedings from the 2010 AGU Ocean Sciences Meeting},
  pages = {22--26},
  abstract = {It is estimated that 20-40\% of heterotrophic bacteria in the ocean are lysed by viruses every day. The lysed bacteria release nitrogen-rich cell contents that can be remineralised by other heterotrophic bacteria. To determine the effect of viruses on ammonium regeneration, two experiments were carried out in August and November 2008. Seawater from Vancouver BC was filtered to remove grazers and phytoplankton larger than 2 mu m. Bacteria were then concentrated, and diluted with virus-free water to reduce virus abundance, or with virus-replete water to recall natural virus abundances. Over two day incubations, virus-replete treatments showed increased ammonium concentrations compared to treatments with viruses removed (differences of 6.77 plus or minus 0.12 mu M and 1.90 plus or minus 0.22 mu M in August and November, respectively). As expected, bacterial cell abundances were lower in the virus-replete treatments. Nanophytoplankton cell abundances and chlorophyll a levels were both higher in the virus-replete treatments, potentially indicating the increased availability of ammonium in the presence of bacterial viruses. Our data support the hypothesis that removal of viruses reduces the amount of viral lysis of heterotrophic bacteria, thereby reducing the amount of dissolved organic nitrogen available to be remineralised by uninfected bacteria. The data suggest that viral lysis plays an important role in marine nitrogen cycling.}
}

