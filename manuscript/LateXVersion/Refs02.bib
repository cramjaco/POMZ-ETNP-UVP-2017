@article{letelierTemporalVariabilityPhytoplankton1993,
  title = {Temporal Variability of Phytoplankton Community Structure-Based on Pigment Analysis},
  author = {Letelier, Ricardo M. and Bidigare, RR and Hebel, DV and Ondrusek, M. and Winn, CD and Karl, David M.},
