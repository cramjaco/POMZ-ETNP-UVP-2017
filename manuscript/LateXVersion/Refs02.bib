@article{fuchsmanDetectionTransientDenitrification2019,
  title = {Detection of {{Transient Denitrification During}} a {{High Organic Matter Event}} in the {{Black Sea}}},
  author = {Fuchsman, Clara A. and Paul, Barbara and Staley, James T. and Yakushev, Evgeniy V. and Murray, James W.},
  date = {2019},
  journaltitle = {Global Biogeochemical Cycles},
