}

@article{mocaliEcologyColdEnvironments2017,
  title = {Ecology of Cold Environments: New Insights of Bacterial Metabolic Adaptation through an Integrated Genomic-Phenomic Approach},
  shorttitle = {Ecology of Cold Environments},
  author = {Mocali, Stefano and Chiellini, Carolina and Fabiani, Arturo and Decuzzi, Silvia and de Pascale, Donatella and Parrilli, Ermenegilda and Tutino, Maria Luisa and Perrin, Elena and Bosi, Emanuele and Fondi, Marco and Lo Giudice, Angelina and Fani, Renato},
  date = {2017-04-12},
