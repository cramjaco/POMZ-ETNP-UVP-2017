@incollection{andersonUnfrozenInterfacialPhase1973,
  title = {The {{Unfrozen Interfacial Phase}} in {{Frozen Soil Water Systems}}},
  booktitle = {Physical {{Aspects}} of {{Soil Water}} and {{Salts}} in {{Ecosystems}}},
  author = {Anderson, D. M. and Tice, A. R.},
  editor = {Hadas, A. and Swartzendruber, D. and Rijtema, P. E. and Fuchs, M. and Yaron, B.},
