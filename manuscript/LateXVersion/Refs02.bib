@article{smrigaIndividualCellDNA2014,
  title = {Individual Cell {{DNA}} Synthesis within Natural Marine Bacterial Assemblages as Detected by ‘Click’ Chemistry},
  author = {Smriga, S and Samo, Tj and Malfatti, F and Villareal, J and Azam, F},
  date = {2014-07-10},
  journaltitle = {Aquatic Microbial Ecology},
