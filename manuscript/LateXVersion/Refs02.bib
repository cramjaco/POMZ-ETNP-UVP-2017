@article{cornwellSedimentWaterNitrogenExchange2015,
  title = {Sediment-{{Water Nitrogen Exchange}} along the {{Potomac River Estuarine Salinity Gradient}}},
  author = {Cornwell, Jeffrey C. and Owens, Michael S. and Boynton, Walter R. and Harris, Lora A.},
  date = {2015-12-11},
