}

@article{giovannoniSeasonalityOceanMicrobial2012,
  title = {Seasonality in {{Ocean Microbial Communities}}},
  author = {Giovannoni, Stephen J and Vergin, Kevin L},
  date = {2012-02-10},
  journaltitle = {Science},
  shortjournal = {Science},
  volume = {335},
