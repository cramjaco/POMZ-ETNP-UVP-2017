  number = {6917}
}

@article{morrisTemporalSpatialResponse2005,
  title = {Temporal and Spatial Response of Bacterioplankton Lineages to Annual Convective Overturn at the {{Bermuda Atlantic Time}}-Series {{Study}} Site},
