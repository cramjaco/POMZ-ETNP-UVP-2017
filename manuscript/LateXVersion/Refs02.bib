@article{jacksonNovelCircularSingleStranded2016,
  title = {Novel {{Circular Single}}-{{Stranded DNA Viruses}} among an {{Asteroid}}, {{Echinoid}} and {{Holothurian}} ({{Phylum}}: {{Echinodermata}})},
