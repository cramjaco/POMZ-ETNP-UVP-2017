  eprinttype = {pmid},
  file = {/home/jacob/Zotero/storage/QGJ54ETF/Weber and Deutsch - 2014 - Local versus basin-scale limitation of marine nitr.pdf;/home/jacob/Zotero/storage/3ME2QNR8/8741.html},
