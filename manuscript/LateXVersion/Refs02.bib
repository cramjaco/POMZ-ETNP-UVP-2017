  doi = {10.1029/LN014p0085},
  url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/LN014p0085},
  urldate = {2019-01-26},
  abstract = {In estuaries and coastal waters, fine particles are transported as aggregates with size and settling properties controlled by coagulation and breakup mechanisms. A review of the separation sciences literature on emulsion and aggregate breakup in turbulent fluids reveals considerable theoretical and experimental uncertainty, but the importance of inhomogeneities in reactor turbulence near walls and rotating elements is recognized. Predictions of droplet and floe sizes have not accounted for this inhomogeneity except through questionable assumptions of breakup kinetics. Available clay coagulation and breakup data in seawater systems indicate that clay aggregates are relatively fragile and low fluid shear rates of 8 s-1 are sufficient to decrease overall removal kinetics because aggregate sizes are limited by breakup. Calculations indicate that aggregate deposition in estuaries is controlled by breakup processes occurring within the viscous sublayer rather than aggregate dynamics calculated from mean fluid properties.},
  file = {/home/jacob/Zotero/storage/U63UZQC6/LN014p0085.html},
  isbn = {978-1-118-66917-4},
  keywords = {Bingham plastic model,Boltzmann constant,Cohesive sediment transport,Estuaries and coastal waters,Fluid shear,Larry Sanford,Quasi-steady coagulation,Steady state droplet and aggregate sizes,Viscous sublayer},
  langid = {english}
}

