}

@article{mazuecosTemperatureControlMicrobial2015,
  title = {Temperature Control of Microbial Respiration and Growth Efficiency in the Mesopelagic Zone of the {{South Atlantic}} and {{Indian Oceans}}},
  author = {Mazuecos, Ignacio P. and Arístegui, Javier and Vázquez-Domínguez, Evaristo and Ortega-Retuerta, Eva and Gasol, Josep M. and Reche, Isabel},
