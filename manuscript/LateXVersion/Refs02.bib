@article{kaddouraThinkPairShare2013,
  title = {Think {{Pair Share}}: {{A Teaching Learning Strategy}} to {{Enhance Students}}' {{Critical Thinking}}},
  shorttitle = {Think {{Pair Share}}},
  author = {Kaddoura, Mahmoud},
